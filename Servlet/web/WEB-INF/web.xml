<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>web1</servlet-name>
        <servlet-class>com.tedu.SecondServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>web1</servlet-name>
        <url-pattern>/web1</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>web2</servlet-name>
        <servlet-class>com.tedu.ResponseDemo</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>web2</servlet-name>
        <url-pattern>/web2</url-pattern>
    </servlet-mapping>
</web-app>